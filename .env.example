# PackEx Environment Variables Example
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/packex_db"

# UploadThing Configuration
# Get your token from https://uploadthing.com/dashboard
UPLOADTHING_TOKEN="your-uploadthing-token-here"

# Application Settings
NODE_ENV=development
SESSION_SECRET=your-strong-random-secret-key

# Optional: CORS settings (for development)
CORS_ORIGIN=http://localhost:5173

# Optional: Logging level
LOG_LEVEL=info
